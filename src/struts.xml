<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	
	<package name="default" extends="struts-default">
		
		<global-results>
			<result name="NaoLogado">/WEB-INF/content/login.jsp</result>
		</global-results>
		
		<action name="loginForm">
			<result>/WEB-INF/content/login.jsp</result>
		</action>
		
		<action name="representantes">
			<result>/WEB-INF/content/listrepresentante.jsp</result>
		</action>
		
		<action name="representanteNew">
			<result>/WEB-INF/content/representante.jsp</result>
		</action>
		
		<action name="products">
			<result>/WEB-INF/content/listproduct.jsp</result>
		</action>
		
		<action name="productNew">
			<result>/WEB-INF/content/product.jsp</result>
		</action>
		
	</package>
	
</struts>
