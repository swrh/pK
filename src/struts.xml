<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="default" extends="struts-default">
		<!-- Default interceptor stack. -->
		<default-interceptor-ref name="paramsPrepareParamsStack"/>

		<action name="representantes">
			<result>/WEB-INF/content/listrepresentante.jsp</result>
		</action>

		<action name="representanteNew">
			<result>/WEB-INF/content/representante.jsp</result>
		</action>

		<action name="product-new">
			<result>/WEB-INF/content/product-new.jsp</result>
		</action>

		<action name="customer-new">
			<result>/WEB-INF/content/customer-new.jsp</result>
		</action>

		<action name="department-new">
			<result>/WEB-INF/content/department-new.jsp</result>
		</action>

		<action name="forgot-password">
			<result>/WEB-INF/content/forgot-password.jsp</result>
		</action>

		<action name="home">
			<result>/WEB-INF/content/menu.jsp</result>
		</action>

		<action name="indication-index" class="com.dasho2.pk.action.IndicationAction" method="list">
			<result name="success">/WEB-INF/pages/indication-index.jsp</result>
			<interceptor-ref name="basicStack"/>
		</action>

		<action name="indication-crud" class="com.dasho2.pk.action.IndicationAction" method="input">
			<result name="success" type="redirectAction">indication-index</result>
			<result name="input">/WEB-INF/pages/indication-form.jsp</result>
			<result name="error">/WEB-INF/pages/error.jsp</result>
		</action>

	</package>

</struts>
