<!DOCTYPE validators PUBLIC "-//OpenSymphony Group//XWork Validator 1.0.2//EN"
       "http://struts.apache.org/dtds/xwork-validator-1.0.2.dtd">
<validators>
  <field name="representative.companyName">
     <field-validator type="requiredstring">
          <message key="É obrigatório indicar o a razão social do representante" />
      </field-validator>
  </field>
  <field name="representative.contactName">
     <field-validator type="requiredstring">
          <message key="É obrigatório indicar o nome para contato com o representante" />
      </field-validator>
  </field>
</validators>
